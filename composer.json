{
    "name": "moirei/laravel-vouchers",
    "description": "Manage ecommerce vouchers",
    "keywords": [
        "moirei",
        "laravel",
        "laravel-vouchers",
        "vouchers",
        "promocodes",
        "coupons",
        "moirei.com"
    ],
    "homepage": "https://github.com/augustusnaz/laravel-vouchers",
    "license": "MIT",
    "authors": [
        {
            "name": "Augustus Okoye",
            "email": "augustusokoye@moirei.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/config": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0",
        "illuminate/database": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0",
        "moirei/laravel-model-data": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.1",
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "~3.5.0|~3.6.0|~3.7.0|~3.8.0"
    },
    "autoload": {
        "psr-4": {
            "MOIREI\\Vouchers\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MOIREI\\Vouchers\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MOIREI\\Vouchers\\VouchersServiceProvider"
            ],
            "aliases": {
                "Vouchers": "MOIREI\\Vouchers\\Facades\\Vouchers"
            }
        }
    }
}
